<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.mapper.common.DishMapper">

    <insert id="insert" parameterType="com.sky.pojo.Dish" useGeneratedKeys="true" keyProperty="id">
        insert into dish
        (name,category_id,price,image,description,status,create_time,update_time,create_user,update_user,make_time)
        values
            (#{name},#{categoryId},#{price},#{image},#{description},#{status},#{createTime},#{updateTime},#{createUser},#{updateUser},#{makeTime})
    </insert>
</mapper>